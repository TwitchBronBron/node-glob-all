{
  "name": "glob-manifest",
  "version": "0.0.2",
  "description": "Another wrapper around node-glob which provides multi-matching. Main feature this package provides is a smart removal of duplicates to make it easy to build ordered file manifests.",
  "main": "index.js",
  "scripts": {
    "prepublish": "./node_modules/coffee-script/bin/coffee -c -o ./ src/index.coffee"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jpillora/node-glob-manifest.git"
  },
  "dependencies": {
    "glob": "~3.1.21",
    "async": "~0.2.6",
    "lodash": "~1.0.1"
  },
  "devDependencies": {
    "coffee-script": "~1.6.2",
    "mocha": "~1.9.0"
  },
  "keywords": [
    "glob",
    "multi",
    "manifest",
    "generation",
    "file"
  ],
  "author": "Jaime Pillora",
  "license": "MIT"
}
